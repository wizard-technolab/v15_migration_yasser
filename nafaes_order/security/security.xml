<odoo>
    <data>
        <record id="module_category_nafaes" model="ir.module.category">
            <field name="name">Nafaes API</field>
            <field name="sequence" type="int">1</field>
        </record>
        <record id="group_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" eval="ref('module_category_nafaes')"/>
            <field name="implied_ids" eval="[(6,0,[ref('base.group_user')])]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
        <record id="group_manager" model="res.groups">
            <field name="name">Manger</field>
            <field name="category_id" eval="ref('module_category_nafaes')"/>
            <field name="implied_ids" eval="[(6,0,[ref('nafaes_order.group_user')])]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
        <record id="group_admin" model="res.groups">
            <field name="name">Admin</field>
            <field name="category_id" eval="ref('module_category_nafaes')"/>
            <field name="implied_ids" eval="[(6,0,[ref('nafaes_order.group_user')])]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
    </data>
</odoo>