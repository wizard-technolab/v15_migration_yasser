<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_score_score_form" model="ir.ui.view">
        <field name="name">view.score.score.form</field>
        <field name="model">score.score</field>
        <field name="arch" type="xml">
            <form string="Score">
                <group>
                    <group>
                        <field name="fuel_evaluate_mini_range" readonly="0"/>
                        <field name="fuel_evaluate_max_range" readonly="0"/>
                        <field name="fuel_evaluate_arabic_reasons" readonly="0"/>
                    </group>
                    <group>
                        <field name="fuel_evaluate_value" readonly="0"/>
                        <field name="fuel_evaluate_english_reasons" readonly="0"/>
                    </group>
                </group>
                <group>
                    <group>
                        <field name="fuel_credit_instruments" readonly="0"/>
                        <field name="fuel_defaults_count" readonly="0"/>
                        <field name="fuel_defaults_amount" readonly="0"/>
                        <field name="fuel_ava_defaults" readonly="0"/>
                        <field name="limit_avar_requested" readonly="0"/>
                    </group>
                    <group>
                        <field name="last_earliest_loan" readonly="0"/>
                        <field name="fuel_total_limits" readonly="0"/>
                        <field name="fuel_total_liabilities" readonly="0"/>
                        <field name="fuel_total_inquiries" readonly="0"/>
                    </group>
                </group>
                <group>
                    <notebook>
                        <page string="Default Score">
                            <field name="default_score" readonly="0">
                                <tree editable="bottom">
                                    <field name="year" readonly="0"/>
                                    <field name="product_type" readonly="0"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </group>
                <group>
                    <notebook>
                        <page string="Enquiries Score">
                            <field name="enquiries_score" readonly="0">
                                <tree editable="bottom">
                                    <field name="year" readonly="0"/>
                                    <field name="product_type" readonly="0"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </group>
                <group>
                    <notebook>
                        <page string="Instrument Score">
                            <field name="instrument_score" readonly="0">
                                <tree editable="bottom">
                                    <field name="Product_type_categorization" readonly="0"/>
                                    <field name="status" readonly="0"/>
                                    <field name="credit_limits" readonly="0"/>
                                    <field name="installment_amounts" readonly="0"/>
                                    <field name="tenure" readonly="0"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </group>
            </form>
        </field>
    </record>
</odoo>