<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_simah_expense_form" model="ir.ui.view">
            <field name="name">view.simah.expense.form</field>
            <field name="model">simah.expense</field>
            <field name="arch" type="xml">
                <form string="Simah Expense">
                    <sheet>
                        <group>
                            <group>
                                <field name="nameEn"/>
                                <field name="valueUsedIncaluculation"/>
                                <field name="outputValue"/>
                            </group>
                            <group>
                                <field name="nameAr"/>
                                <field name="valueDeclaredByCustomer"/>
                                <field name="isVerified"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_simah_expense" model="ir.actions.act_window">
            <field name="name">simah Expense</field>
            <field name="res_model">simah.expense</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>