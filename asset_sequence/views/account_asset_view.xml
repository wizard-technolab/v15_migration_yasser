<odoo>
    <record id="view_account_asset_form_inherit_code" model="ir.ui.view">
        <field name="name">account.asset.form.inherit.asset.code</field>
        <field name="model">account.asset</field>
        <field name="inherit_id" ref="account_asset.view_account_asset_form"/>
        <field name="priority">32</field>
        <field name="arch" type="xml">
            <xpath expr="//sheet/div[@class='oe_title']" position="before">
                <group>
                    <field name="asset_code" readonly="1"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
